{
  "name": "followkaro-server",
  "version": "1.0.0",
  "description": "Backend API for FollowKaro - Social Engagement Platform",
  "main": "dist/main.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --project tsconfig.json -r tsconfig-paths/register src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "lint": "eslint src/**/*.ts",
    "seed:superadmin": "ts-node --project tsconfig.json -r tsconfig-paths/register src/scripts/super-admin.seed.ts",
    "seed:fonts": "ts-node --project tsconfig.json -r tsconfig-paths/register src/scripts/font.seed.ts",
    "seed:themes": "ts-node --project tsconfig.json -r tsconfig-paths/register src/scripts/theme.seed.ts"
  },
  "keywords": [
    "social-engagement",
    "campaigns",
    "earnings"
  ],
  "author": "FollowKaro Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "http-status": "^2.1.0",
    "inquirer": "^12.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
